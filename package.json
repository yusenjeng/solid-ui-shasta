{
  "version": "0.0.2",
  "name": "solid-ui-shasta",
  "description": "solid-ui-shasta",
  "license": "MIT",
  "author": "yusenjeng",
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yusenjeng/solid-ui-shasta"
  },
  "homepage": "https://github.com/yusenjeng/solid-ui-shasta/blob/main/README.md",
  "bugs": {
    "url": "https://github.com/yusenjeng/solid-ui-shasta/issues"
  },
  "files": [
    "dist"
  ],
  "private": false,
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "browser": {},
  "exports": {
    "solid": {
      "development": "./dist/dev.jsx",
      "import": "./dist/index.jsx"
    },
    "development": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/dev.js"
      },
      "require": "./dist/dev.cjs"
    },
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "require": "./dist/index.cjs"
  },
  "typesVersions": {},
  "scripts": {
    "dev": "vite serve dev --host",
    "build": "tsup",
    "test": "concurrently pnpm:test:*",
    "test:client": "vitest",
    "test:ssr": "pnpm run test:client --mode ssr",
    "prepublishOnly": "pnpm build",
    "format": "prettier --ignore-path .gitignore -w \"src/**/*.{js,ts,json,css,tsx,jsx}\" \"dev/**/*.{js,ts,json,css,tsx,jsx}\"",
    "update-deps": "pnpm up -Li",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "solid-js": "^1.7.5"
  },
  "devDependencies": {
    "@solid-devtools/debugger": "^0.22.4",
    "@solidjs/router": "^0.8.3",
    "concurrently": "^8.2.1",
    "esbuild": "^0.19.3",
    "esbuild-plugin-solid": "^0.5.0",
    "jsdom": "^22.1.0",
    "prettier": "3.0.3",
    "solid-devtools": "^0.27.7",
    "solid-js": "^1.7.12",
    "tsup": "^7.2.0",
    "tsup-preset-solid": "^2.1.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-solid": "^2.7.0",
    "vitest": "^0.34.4"
  },
  "keywords": [
    "solid"
  ],
  "packageManager": "pnpm@7.22.0",
  "dependencies": {
    "solid-styled-components": "^0.28.5"
  }
}
